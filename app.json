{
  "expo": {
    "name": "Good: Minimal Activity Track",
    "slug": "minimalLifeTracker",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f9f9f9"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#f9f9f9"
      },
      "package": "com.junyan.minimalLifeTracker",
      "versionCode": 4,
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Track you life progress in the most minimal way. Remember, you Define what the black and white boxes are. Long press for placing white box, Double tap for placing black box.."
  }
}
